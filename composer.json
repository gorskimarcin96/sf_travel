{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^3",
    "doctrine/doctrine-bundle": "^2",
    "doctrine/doctrine-migrations-bundle": "^3",
    "doctrine/orm": "^2",
    "martin-georgiev/postgresql-for-doctrine": "^2.3",
    "nelmio/cors-bundle": "^2",
    "phpdocumentor/reflection-docblock": "^5",
    "phpstan/phpdoc-parser": "^1",
    "symfony/amqp-messenger": "^6.4",
    "symfony/asset": "^6.4",
    "symfony/console": "^6.4",
    "symfony/css-selector": "^6.4",
    "symfony/doctrine-messenger": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/expression-language": "^6.4",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/monolog-bundle": "^3",
    "symfony/panther": "^2",
    "symfony/property-access": "^6.4",
    "symfony/property-info": "^6.4",
    "symfony/runtime": "^6.4",
    "symfony/security-bundle": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/yaml": "^6.4"
  },
  "require-dev": {
    "ext-gd": "*",
    "behat/behat": "^3",
    "coduo/php-matcher": "^6",
    "friends-of-behat/symfony-extension": "^2",
    "friendsofphp/php-cs-fixer": "^3",
    "phpstan/phpstan": "^1",
    "phpstan/phpstan-symfony": "^1",
    "phpunit/phpunit": "^9",
    "rector/rector": "*",
    "symfony/browser-kit": "^6.4",
    "symfony/maker-bundle": "^1",
    "symfony/phpunit-bridge": "^6.4"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "tests": [
      "@phpunit",
      "@rector",
      "@phpstan",
      "@csfix",
      "@behat"
    ],
    "test-database": [
      "./bin/console cache:clear --env=test -q",
      "./bin/console doctrine:schema:validate -n",
      "./bin/console doctrine:database:drop --env=test -q --force --if-exists",
      "./bin/console doctrine:database:create --env=test -q",
      "./bin/console doctrine:migration:migrate --env=test -q -n"
    ],
    "phpunit": [
      "@test-database",
      "./bin/phpunit",
      "./bin/console app:test:summary-image"
    ],
    "phpstan": [
      "php -d memory_limit=-1 vendor/bin/phpstan analyse -c phpstan.neon src",
      "php -d memory_limit=-1 vendor/bin/phpstan analyse -c phpstan.neon tests"
    ],
    "rector": [
      "vendor/bin/rector process src --clear-cache"
    ],
    "csfix": [
      "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix"
    ],
    "behat": "php -d memory_limit=-1 ./vendor/bin/behat -f progress"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^6.4"
    }
  }
}
